#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include<string>
using namespace std;
int main()
{
    long int n,s=0;
    long long int a,p=1;
      cin>>n;
   
    for(int i=0;i<n;i++)
    {
        cin>>a;
        while(a%10==0)
        {
            s+=1;
            a/=10;
        }
        p*=a;
    }
    cout<<p;
    for(int k=0; k<s ;k++)
    {
      cout<<"0";
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}

